[global]
DEFAULT_TEMPLATE=aguatest-testcluster
ENABLE_EXPERIMENTAL=True

[cluster aguatest-testcluster]
AVAILABILITY_ZONE=us-east-1a
CLUSTER_SIZE=1
CLUSTER_USER=sgeadmin
KEYNAME=id_rsa-aguatest-key
NODE_IMAGE_ID=ami-11c67678
NODE_INSTANCE_TYPE=t1.micro
PLUGINS=automount,sge,startup

[aws info]
AWS_ACCESS_KEY_ID=AZAZAZAZAZAZAZAZ
AWS_SECRET_ACCESS_KEY=a0a0a0a0a0a0a0a0a0a0a0a0aa0a0a0a0a0a0a0a0a0
AWS_USER_ID=000000000000

[key id_rsa-aguatest-key]
KEYNAME=id_rsa-aguatest-key
KEY_LOCATION=/agua/t/bin/Agua/Workflow/outputs/startstarcluster/aguatest/.starcluster/id_rsa-aguatest-key

[plugin automount]
cluster=aguatest-testcluster
interval=30
mountdport=32767
mountpoints=/agua,/data,/nethome
nfsport=2049
portmapport=111
privatekey=/agua/conf/.ec2/aguatest/private.pem
publiccert=/agua/conf/.ec2/aguatest/public.pem
setup_class=automount.NfsShares
sourcedirs=/agua,/data,/nethome

[plugin sge]
cell=aguatest-testcluster
execdport=56322
privatekey=/agua/conf/.ec2/aguatest/private.pem
publiccert=/agua/conf/.ec2/aguatest/public.pem
qmasterport=56321
root=/opt/sge6
setup_class=sge.CreateCell
slots=1

[plugin startup]
cell=aguatest-testcluster
headnodeid=
installdir=/agua
privatekey=/agua/conf/.ec2/aguatest/private.pem
publiccert=/agua/conf/.ec2/aguatest/public.pem
root=/agua
setup_class=startup.StartUp
version=0.8.0-alpha.1+build13

